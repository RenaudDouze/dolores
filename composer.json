{
    "name": "aptoma/silex-bootstrap",
    "description": "Basic bootstrap for Silex based projects. Use as template for getting started quickly.",
    "authors": [
        {
            "name": "Gunnar Lium",
            "email": "gunnar@aptoma.com"
        }
    ],
    "autoload": {
        "psr-0": {
            "App": "src/",
            "Aptoma": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:aptoma/silex-extras.git"
        }
    ],
    "require": {
        "silex/silex": "~1.1",
        "aptoma/silex-extras": "master@dev",
        "monolog/monolog": "~1.3",
        "symfony/twig-bridge": "~2.3",
        "symfony/monolog-bridge": "~2.3",
        "symfony/browser-kit": "~2.3",
        "symfony/css-selector": "~2.3"
    },
    "require-dev": {
        "phpunit/phpunit": "~3.7",
        "pdepend/pdepend": "~1.1",
        "phpmd/phpmd": "~1.4",
        "squizlabs/php_codesniffer": "~1.4"
    },
    "minimum-stability": "stable"
}
